#include <iostream>
using namespace std;
int main()

{
      int x;

    cout << "Enter an integer x: ";
    cin >> x;

    cout << "++x = " << ++x << endl;   
    cout << "x++ = " << x++ << endl;   
    cout << "--x = " << --x << endl;   
    cout << "x-- = " << x-- << endl;   

 


 

    return 0 ;
}
